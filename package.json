{
  "name": "excel-creator",
  "version": "1.0.0",
  "description": "Create beautiful Excel spreadsheets from JSON or Markdown with rich styling and formulas",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "excel-creator": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write 'src/**/*.ts'",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "excel",
    "xlsx",
    "spreadsheet",
    "json",
    "markdown",
    "converter",
    "exceljs",
    "cli",
    "typescript"
  ],
  "author": "Lindsay Smith",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wapdat/excel-creator"
  },
  "bugs": {
    "url": "https://github.com/wapdat/excel-creator/issues"
  },
  "homepage": "https://github.com/wapdat/excel-creator#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "chalk": "^5.5.0",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.0",
    "exceljs": "^4.4.0",
    "marked": "^16.1.2",
    "ora": "^8.2.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
